package Com.code;

public class Add_Two_No {

	public static void main(String[] args) {

		int a=5;
		int b=6;
		
		int add=a+b;
		int sub=a-b;
		int mul=a*b;
		int divide=a/b;
		int mod=a^b;
		
		System.out.println("A+B =Add "+add);
		System.out.println("A-B =Sub "+sub);
		System.out.println("A*B = Mul "+mul);
		System.out.println("A/B =divide "+divide);
		System.out.println("A%B =Mod "+mod);
		
		
	}

}
